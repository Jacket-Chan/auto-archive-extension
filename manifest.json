{
  "manifest_version": 2,
  "name": "Auto-Archive",
  "version": "1.0",
  "description": "This is an extension that automatically archives (or at least attempts to) every page you visit.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "author": "jacketchan",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  },
  "homepage_url": "https://jacket-chan.github.io",
  "incognito": "spanning",
  "minimum_chrome_version": "0.2.149",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "aString"
  },
  "permissions": ["tabs"],
  "short_name": "Auto-Archive",
  "update_url": "http://path/to/updateInfo.xml",
  "version_name": "1.0 Gamma"
}
