{
  "manifest_version": 2,
  "name": "Auto-Archive",
  "version": "1.0",
   "browser_action": {
   },
  "background": {
    "persistent" : true,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
   "tabs"
 ]
}
